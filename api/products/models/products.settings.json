{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Products"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "cover_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "shop": {
      "model": "shop"
    },
    "price": {
      "type": "float",
      "default": 0,
      "min": 0
    },
    "categories": {
      "collection": "product-category",
      "via": "products",
      "dominant": true
    }
  }
}
